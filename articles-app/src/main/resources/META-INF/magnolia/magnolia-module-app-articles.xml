<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module SYSTEM "module.dtd" >
<module>
  <name>app-articles</name>
  <displayName>App Articles</displayName>
  <description>Articles Content</description>
  <version>${project.version}</version>

  <dependencies>
    <dependency>
      <name>core</name>
      <version>5.4/*</version>
    </dependency>
    <dependency>
      <name>ui-admincentral</name>
      <version>5.4/*</version>
    </dependency>
    <dependency>
      <name>ui-contentapp</name>
      <version>5.4/*</version>
      <optional>true</optional>
    </dependency>
    <dependency>
      <name>dam-app</name>
      <version>2.1/*</version>
    </dependency>
  </dependencies>


  <repositories>
    <repository>
      <name>magnolia</name>
      <workspaces>
        <workspace>articles</workspace>
      </workspaces>
      <nodeTypeFile>/mgnl-nodetypes/app-articles-nodetypes.xml</nodeTypeFile>
    </repository>
  </repositories>

</module>