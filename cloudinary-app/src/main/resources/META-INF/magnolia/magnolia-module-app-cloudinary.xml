<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module SYSTEM "module.dtd" >
<module>
  <name>app-cloudinary</name>
  <displayName>App Cloudinary</displayName>
  <description>Cloudinary DAM</description>
  <class>com.sharecare.cms.cloudinary.CloudinaryIntegrationModule</class>
  <versionHandler>com.sharecare.cms.publishing.commons.bootstrap.ConfigVersionHandler</versionHandler>
  <version>1.0</version>


  <components>
    <id>main</id>
    <component>
      <type>com.sharecare.cms.cloudinary.dam.CloudinaryClientServiceConnector</type>
      <implementation>com.sharecare.cms.cloudinary.dam.CloudinaryClientServiceConnector</implementation>
      <scope>singleton</scope>
    </component>
  </components>

  <dependencies>
    <dependency>
      <name>core</name>
      <version>5.4/*</version>
    </dependency>
    <dependency>
      <name>ui-admincentral</name>
      <version>5.4/*</version>
    </dependency>
    <dependency>
      <name>ui-contentapp</name>
      <version>5.4/*</version>
      <optional>true</optional>
    </dependency>
    <dependency>
      <name>dam-app</name>
      <version>2.1/*</version>
    </dependency>
    <dependency>
      <name>enterprise</name>
      <version>5.4/*</version>
    </dependency>
  </dependencies>


</module>